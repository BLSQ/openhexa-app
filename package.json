{
  "name": "openhexa-frontend",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "npm-run-all --parallel next 'codegen -- --watch'",
    "next": "node server/index.mjs",
    "build": "next build",
    "build:ci": "CI=1 next build",
    "start": "NODE_ENV=production node server/index.mjs",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint --dir src",
    "format": "prettier --write src",
    "prepare": "husky install",
    "schema": "npx -y get-graphql-schema http://localhost:8000/graphql/ > schema.graphql",
    "codegen": "find ./src -type f -name \"*.generated.tsx\" -delete && graphql-codegen --config codegen.yml",
    "i18n:extract": "i18next",
    "ladle": "npx ladle serve",
    "ladle:preview": "ladle build && ladle preview 61000"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "graphql": {
    "schema": "./schema.graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-python": "^6.1.2",
    "@headlessui/react": "1.7.10",
    "@heroicons/react": "^2.0.16",
    "@ladle/react": "^2.10.1",
    "@popperjs/core": "^2.11.6",
    "@sentry/nextjs": "^7.43.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-table": "^8.7.9",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.15.1",
    "@types/react": "^18.0.28",
    "@types/uuid": "^9.0.1",
    "@uiw/react-codemirror": "^4.19.9",
    "axios": "^1.3.4",
    "clsx": "^1.2.1",
    "codemirror": "^6.0.1",
    "codemirror-lang-r": "^0.1.0-2",
    "cron-parser": "^4.8.1",
    "cronstrue": "^2.23.0",
    "deepmerge": "^4.3.0",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "filesize": "^10.0.6",
    "graphql": "^16.6.0",
    "i18next": "^22.4.11",
    "isomorphic-unfetch": "^4.0.2",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "luxon": "^3.3.0",
    "next": "^13.2.4",
    "next-i18next": "^13.2.2",
    "nextjs-progressbar": "^0.0.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hotkeys-hook": "^4.3.8",
    "react-i18next": "^12.2.0",
    "react-markdown": "^8.0.5",
    "react-popper": "^2.3.0",
    "react-popper-tooltip": "^4.4.2",
    "react-table": "^7.8.0",

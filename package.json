    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/lodash.merge": "^4.6.7",
    "@types/luxon": "^3.2.0",
    "@types/react-table": "^7.7.14",
    "@types/sync-fetch": "^0.4.0",

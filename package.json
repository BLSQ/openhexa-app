{
  "name": "openhexa-frontend",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "npm-run-all --parallel next 'codegen -- --watch'",
    "next": "node scripts/server.mjs",
    "build": "next build",
    "build:ci": "CI=1 next build",
    "start": "NODE_ENV=production node scripts/server.mjs",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint --dir src",
    "format": "prettier --write src",
    "prepare": "husky install",
    "schema": "npx get-graphql-schema http://localhost:8000/graphql/ > schema.graphql",
    "codegen": "find ./src -type f -name \"*.generated.tsx\" -delete && graphql-codegen --config codegen.yml",
    "i18n:extract": "i18next",
    "ladle": "npx ladle serve"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "graphql": {
    "schema": "./schema.graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@codemirror/lang-json": "^6.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.4.1",
    "@headlessui/react": "1.6.6",
    "@heroicons/react": "^1.0.6",
    "@ladle/react": "^2.4.2",
    "@popperjs/core": "^2.11.6",
    "@sentry/nextjs": "^7.12.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@tanstack/react-table": "^8.5.13",
    "@types/jest": "^29.0.1",
    "@types/lodash": "^4.14.185",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.7.17",
    "@types/react": "^18.0.19",
    "@types/uuid": "^8.3.4",
    "@uiw/react-codemirror": "^4.12.3",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "codemirror": "^6.0.1",
    "deepmerge": "^4.2.2",
    "dropzone": "^6.0.0-beta.2",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "express-http-proxy": "^1.6.3",
    "filesize": "^9.0.11",
    "graphql": "^16.6.0",
    "husky": "^8.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "luxon": "^3.0.3",
    "next": "^12.3.0",
    "next-i18next": "^12.0.1",
    "nextjs-progressbar": "^0.0.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.2",
    "react-markdown": "^8.0.3",
    "react-popper": "^2.3.0",
    "react-popper-tooltip": "^4.4.2",
    "react-table": "^7.8.0",
    "typescript": "^4.7.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@next/eslint-plugin-next": "^12.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.13",
    "@types/lodash.merge": "^4.6.7",
    "@types/luxon": "^3.0.1",
    "@types/react-table": "^7.7.12",
    "autoprefixer": "10.4.9",
    "babel-jest": "^29.0.3",
    "i18next-parser": "^6.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8"
  },
  "version": "0.6.2"
}

{
  "name": "openhexa-frontend",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "npm-run-all --parallel next 'codegen -- --watch'",
    "next": "node server/index.mjs",
    "build": "next build",
    "build:ci": "CI=1 next build",
    "start": "NODE_ENV=production node server/index.mjs",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint --dir src",
    "format": "prettier --write src",
    "prepare": "husky install",
    "schema": "npx -y get-graphql-schema http://localhost:8000/graphql/ > schema.graphql",
    "codegen": "find ./src -type f -name \"*.generated.tsx\" -delete && graphql-codegen --config codegen.yml",
    "i18n:extract": "i18next",
    "ladle": "npx ladle serve",
    "ladle:preview": "ladle build && ladle preview 61000"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "graphql": {
    "schema": "./schema.graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@codemirror/lang-json": "^6.0.1",
    "@headlessui/react": "1.7.4",
    "@heroicons/react": "^2.0.13",
    "@ladle/react": "^2.4.5",
    "@popperjs/core": "^2.11.6",
    "@sentry/nextjs": "^7.17.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@tanstack/react-table": "^8.5.22",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.187",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/uuid": "^8.3.4",
    "@uiw/react-codemirror": "^4.12.4",
    "axios": "^1.1.3",
    "clsx": "^1.2.1",
    "codemirror": "^6.0.1",
    "deepmerge": "^4.2.2",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "filesize": "^10.0.5",
    "graphql": "^16.6.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "luxon": "^3.1.0",
    "next": "^13.0.2",
    "next-i18next": "^12.1.0",
    "nextjs-progressbar": "^0.0.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hotkeys-hook": "^3.4.7",
    "react-markdown": "^8.0.3",
    "react-popper": "^2.3.0",
    "react-popper-tooltip": "^4.4.2",
    "react-table": "^7.8.0",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-apollo": "^3.3.6",
    "@graphql-tools/mock": "^8.7.10",
    "@graphql-tools/schema": "^9.0.8",
    "@next/eslint-plugin-next": "^13.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.14",
    "@types/lodash.merge": "^4.6.7",
    "@types/luxon": "^3.0.3",
    "@types/react-table": "^7.7.12",
    "@types/sync-fetch": "^0.4.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.2.2",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "i18next-parser": "^6.6.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.0.3",
    "next-router-mock": "^0.7.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
